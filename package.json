{
  "name": "jackstands",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "check-typescript": "tsc --noEmit",
    "check-eslint": "eslint './src/**/*{js,ts,jsx,tsx}'",
    "lint": "yarn check-typescript && yarn check-eslint",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint './src/**/*{js,ts,jsx,tsx}' --fix",
      "prettier --write './src/**/*{js,ts,jsx,tsx}'"
    ]
  },
  "keywords": [
    "react-native",
    "expo",
    "typescript",
    "template"
  ],
  "author": "Vladimir Vovk",
  "license": "MIT",
  "homepage": "https://github.com/vladimir-vovk/expo-ts",
  "bugs": {
    "url": "https://github.com/vladimir-vovk/expo-ts/issues"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@morrowdigital/watermelondb-expo-plugin": "^2.1.0",
    "@nozbe/watermelondb": "^0.25.5",
    "@nozbe/with-observables": "^1.5.0-3",
    "@react-native-community/checkbox": "^0.5.15",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@rneui/themed": "^4.0.0-rc.7",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "expo": "~47.0.14",
    "expo-build-properties": "~0.4.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-native": "0.70.8",
    "react-native-check-box": "^2.1.7",
    "react-native-paper": "^5.7.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-sqlite3": "^8.3.0",
    "eslint": "^8.31.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0"
  },
  "private": true
}
